<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
    <children>
        <BorderPane>
            <center>
                <fx:include fx:id="CentralFrame" source="AllMusicsCenterView.fxml" />
            </center>
             <left>
                <VBox BorderPane.alignment="TOP_RIGHT">
                    <children>
                        <fx:include fx:id="Profile" source="ProfileSideView.fxml" />
                        <fx:include fx:id="MyMusicsSide" source="MyMusicsSideView.fxml" />
                    </children>
                </VBox>
             </left>
             <right>
                <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
             </right>
        </BorderPane>
    </children>
</AnchorPane>
