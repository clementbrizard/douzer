<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane styleClass="left-side-view" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.OnlineUsersListController">
    <children>
        <VBox styleClass="to-center">
            <StackPane fx:id="pnOnlineUser" styleClass="left-side-pane, to-center">
                <VBox StackPane.alignment="CENTER">
                    <HBox styleClass="to-center">
                        <Label styleClass="left-view-title" text="Utilisateur en ligne" />
                    </HBox>
                    <ListView fx:id="lvwOnlineUsers" styleClass="left-side-lv" />
                </VBox>
            </StackPane>
        </VBox>
   </children>
</AnchorPane>
